{
  "scripts": {
    "dev": "nodemon ./src/server/index.ts",
    "test": "cross-env NODE_ENV=test jest --testTimeout=5000",
    "lint:js": "eslint --ext \".js,.ts\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix",
    "prepare": "husky install"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testMatch": [
      "**/*.test.(ts|tsx)"
    ]
  },
  "name": "express-server",
  "version": "0.2.2",
  "main": "./src/app.ts",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@types/node": "^18.15.11",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest": "^29.6.2",
    "json-cyclic": "^1.0.2",
    "mysql2": "^3.1.2",
    "nodemon": "^2.0.20",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "save-dev": "^0.0.1-security",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typeorm": "0.3.15",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache",
    "*.**": "prettier --check --ignore-unknown"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/sinon": "^10.0.16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.6.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.0",
    "ts-jest": "^29.1.1"
  }
}
